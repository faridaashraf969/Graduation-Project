@model Course

@{
    ViewBag.Title = "Watch Course";
}

<h2>Watch Course: @Model.Topic</h2>
<hr />
<h3>Here Is Your Course!!</h3>
<br />


<div>
    <video width="900" controls>
        <source src="@Url.Content(@Model.VideoContentUrl)" type="video/mp4">
        Your browser does not support the video tag.
    </video>
</div>
<a asp-action="ClientHome" asp-controller="Client" class="btn btn-primary">Back To Home</a>


