@model SessionRequest
@{
    ViewData["Title"] = "Details";
}


<h1>Session Request Details</h1>
<p><strong>Description:</strong> @Model.Description</p>
<!-- Display other session request details -->

<h2>Proposals</h2>
@if (Model.Proposals.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Photographer</th>
                <th>Proposal Text</th>
                <!-- Add other proposal details -->
            </tr>
        </thead>
        <tbody>
            @foreach (var proposal in Model.Proposals)
            {
                <tr>
                    <td>@proposal.PhotographerId</td>
                    <td>@proposal.ProposalText</td>
                    <!-- Display other proposal details -->
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No proposals submitted yet.</p>
}
