@model SessionBid
<link href="~/css/newsessionbid.css" rel="stylesheet" />
@{
    ViewData["Title"] = "NewSessionBid";
}

<h1>NewSessionBid</h1>
<form method="post" asp-action="NewSessionBid">
    <div class="form-group">
        <label asp-for="Description"></label>
        <input asp-for="Description" class="form-control" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SessionDate"></label>
        <input asp-for="SessionDate" class="form-control" />
        <span asp-validation-for="SessionDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Price"></label><link href="~/css/newsessionbid.css" rel="stylesheet" />
        <input asp-for="Price" class="form-control" />
        <span asp-validation-for="Price" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Location"></label>
        <input asp-for="Location" class="form-control" />
        <span asp-validation-for="Location" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Details"></label>
        <input asp-for="Details" class="form-control" />
        <span asp-validation-for="Details" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SessionType"></label>
        <input asp-for="SessionType" class="form-control" />
        <span asp-validation-for="SessionType" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
